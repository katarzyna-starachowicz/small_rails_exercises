<h1>Events created by you:</h1>
<ul>
  <% @created_events.each do |event| %>
    <li>
      <%= event.title %>
      <%= event.date %>
      <%= event.description %> 
    </li>
  <% end %>
</ul>
<br>
<h1>Upcoming events:</h1>
<ul>
  <% @upcoming_events.each do |event| %>
    <li>
      <%= event.title %>
      <%= event.date %>
      <%= event.description %> 
    </li>
  <% end %>
</ul>
<br>
<h1>Previously attended events:</h1>
<ul>
  <% @previous_events.each do |event| %>
    <li>
      <%= event.title %>
      <%= event.date %>
      <%= event.description %> 
    </li>
  <% end %>
</ul>
